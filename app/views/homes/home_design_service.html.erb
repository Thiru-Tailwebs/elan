  </section>

  <section class="white-bg py-40">
    <div class="container">
      <div class="heading-1 fs-35 fs-25-mob black-1 text-center" data-aos="fade-up">
        Choose By Products
      </div>
      <div class="mt-10 d-none-mob">
        <div class="row">
          <% @categories.each do |data| %>
            <div class="col-lg-4 col-md-4 col-sm-6 col-12 mt-20" data-aos="flip-down">
              <a href="<%= show_product_path(data) %>">
                <img src="<%= data.image2_url(:medium) %>" class="w-100p">
                <div class="collection-position" style="background: none;"></div>
                <div class="heading-8 white fs-14-mob position-4 text-center text-upper">
                  <%= data.name %>
