        <div class="mx-10 mx-0-mob">
          <div class="row">
            <% @categories.each do |data|%>
              <div class="mt-30 col-4">
                <div class="nav-item mob-head">
                  <a class="nav-link" href="<%= show_product_path(data) %>">
                    <span>
                      <%= data.name %>
                    </span>
