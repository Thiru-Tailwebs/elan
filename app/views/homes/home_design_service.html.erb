<style>
  ::placeholder {
    font-family: 'Jost', sans-serif !important;
    font-size: 16px !important;
    font-weight: 400 !important;
    color: #787878 !important;
  }

  .form-control.drop {
    font-family: 'Jost', sans-serif !important;
    font-size: 16px !important;
    font-weight: 400 !important;
    color: #787878 !important;
  }

  .form-control:focus {
    border-color: #C2C2C2 !important;
    box-shadow: none !important;
  }

  .form-control {
    height: calc(1.5em + 0.75rem + 10px) !important;
  }

  .modal {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.5);
    justify-content: center;
    align-items: center;
  }

  .modal.active {
    display: flex;
  }

  .modal.active .white-bg {
    width: 30%;
    /* When the modal is active, set to 30% */
  }

  @media (min-width: 992px) and (max-width: 1199.98px) {
    .modal.active .white-bg {
      width: 50%;
      /* When the modal is active, set to 30% */
    }
  }

  @media (max-width: 576px){
    .modal.active .white-bg {
      width: 70%;
      /* When the modal is active, set to 30% */
    }
  }

  .close-modal {
    position: absolute;
    top: 10px;
    right: 10px;
    font-size: 20px;
    cursor: pointer;
    color: #000000;
    z-index: 111;
    /* Adjust color as needed */
  }
</style>

<style>
  .accordion {
    margin-top: 40px;
  }

  .accordion .card {
    border-radius: 16px !important;
    background: #FFF;
    box-shadow: 0px 4px 4px 0px rgba(0, 0, 0, 0.25);
    margin-bottom: 20px;
    padding: 30px;
  }

  .accordion .card-head h2 {
    /* background: url('/images/faq/acc-down.png') no-repeat calc(100% - 10px) center;
  background-size: 20px; */
    cursor: pointer;
    font-size: 18px;
    font-family: 'Jost', sans-serif;
    color: #3A3A3A;
    font-weight: 600;
    width: 80%;
  }

  /* .accordion .card h2.collapsed {
    background-image: url('/images/faq/acc-down.png');
  } */

  .accordion .card-body {
    color: #515151;
    font-family: 'Jost', sans-serif;
    font-size: 16px;
    font-style: normal;
    font-weight: 400;
    line-height: 30px;
  }

  .card-head {
    position: relative;
  }

  .card-head::after {
    content: '';
    background-image: url('/images/faq/grey-arrow.png');
    /* Replace with the actual path to your arrow image */
    background-size: contain;
    display: inline-block;
    width: 20px;
    /* Adjust the width as needed */
    height: 20px;
    /* Adjust the height as needed */
    position: absolute;
    right: 10px;
    top: 50%;
    transform: translateY(-50%);
    cursor: pointer;
  }

  .card-head.collapsed::after {
    background-image: url('/images/faq/acc-down.png');
  }

  @media (max-width: 767px) {
    .accordion .card {
      padding: 15px;
    }

    .accordion .card-head h2 {
      font-size: 14px;
    }

    .accordion .card-body {
      font-size: 14px;
      line-height: 20px;
      padding: 0.7rem !important;
    }

  }
</style>

<style>
  .mob-head .nav-link {
    padding: 0px !important;
    font-size: 10px !important;
    text-align: center;
    color: #2D2D2D !important;
    text-align: center;
    font-family: 'Jost', sans-serif;
    font-size: 14px;
    font-style: normal;
    font-weight: 500;
    text-transform: uppercase;
  }

  .mob-head span {
    display: block !important;
  }

  .mob-head .nav-link:hover {
    color: #A7833D !important;
  }

  
</style>

<script>
  $(document).ready(function () {
    $(".showModal").click(function () {
      $("#modal").addClass("active");
    });

    $("#closeModal").click(function () {
      $("#modal").removeClass("active");
    });

  });

  jQuery(document).ready(function($) {
    $('#myFormDS').submit(function(e) {
      e.preventDefault(); // Prevent the default form submission

      // Get form data
      var formData = {
        name: $('#name').val(),
        phone: $('#phone').val(),
        email: $('#email').val(),
        area: $('#area').val(),
        city: $('#city').val()
      };

      var csrfToken = $('meta[name=csrf-token]').attr('content');

      // Send AJAX request
      $.ajax({
        type: 'POST',
        url: '<%= send_design_service_mail_path %>',
        headers: {
          'X-CSRF-Token': csrfToken
        },
        data: formData,
        success: function(response) {
          // Assuming your modal has the ID "myModal"
          $('#myModal1').show();
        },
        error: function(error) {
          console.error('Error submitting the form:', error);
        }
      });
    });

    $('#myFormDS1').submit(function(e) {
      e.preventDefault(); // Prevent the default form submission

      // Get form data
      var formData = {
        name: $('#name1').val(),
        phone: $('#phone1').val(),
        email: $('#email1').val(),
        area: $('#area1').val(),
        city: $('#city1').val()
      };

      var csrfToken = $('meta[name=csrf-token]').attr('content');

      // Send AJAX request
      $.ajax({
        type: 'POST',
        url: '<%= send_design_service_mail_path %>',
        headers: {
          'X-CSRF-Token': csrfToken
        },
        data: formData,
        success: function(response) {
          // Assuming your modal has the ID "myModal"
          $('#myModal1').show();
        },
        error: function(error) {
          console.error('Error submitting the form:', error);
        }
      });
    });

    // Close modal when the close button is clicked
    $('#closeModal1').click(function() {
      $('#myModal1').hide();
      $('#myFormDS')[0].reset();
      $('#myFormDS1')[0].reset();
      $("#modal").removeClass("active");
      // location.reload();
    });

  });

</script>
