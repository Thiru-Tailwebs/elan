    <div class="bg-shadow-1 h-100vh h-100h-md h-80vh-mob">
      
      <div class="container-fluid">
        <div class="h-100vh h-80vh-lg h-100h-md h-80vh-mob m-auto d-flex align-items-center">
          <div class="px-20 px-0-mob mt-50 mt-0-mob zoom">
            <div class="row align-items-center">
              <div class="col-lg-6" data-aos="zoom-out">
                <div class="heading-3 text-upper fs-25 fs-18-mob white">
                  Unleash Designer Luxury in Your Home
                </div>
                <div class="mt-20">
                  <div class="heading-1 fs-35 fs-25-mob">
                    Indulge in Personalized Home Design with our Premium Soft Furnishings – Where Every Detail Matters
                  </div>
                </div>
              </div>

              <div class="col-lg-5 offset-lg-1 mt-40-md" data-aos="zoom-in">
                <div class="white-bg p-15 br-10 position-1-mob w-95p-mob box-shadow-mob">
                  <div class="heading-8">
                    Get in touch!
                  </div>

                  <form class="mt-20" id="myFormDS">
                    <div class="form-group">
                      <input type="text" class="form-control" name="name" id="name" placeholder="Name" required>
                    </div>
                    <div class="form-group">
                      <input type="tel" class="form-control" name="phone" id="phone" placeholder="Phone Number" required>
                    </div>
                    <div class="form-group">
                      <input type="email" class="form-control" name="email" id="email" placeholder="Email Address" required>
                    </div>
                    <div class="form-group">
                      <select class="form-control drop" name="area" id="area" required>
                        <option value="" disabled selected>Select Category</option>
                        <option value="Fabrics & Drapery">Fabrics & Drapery</option>
                        <option value="furniture">Furniture</option>
                        <option value="Upholstery">Upholstery</option>
                        <option value="Accessories">Accessories</option>
                        <option value="Bath & Bed Linen">Bath & Bed Linen</option>
                        <option value="Floor Coverings">Floor Coverings</option>
                        <option value="Wall Coverings">Wall Coverings</option>
                        <option value="Trims & Tassels">Trims & Tassels</option>
                        <option value="Mattress">Mattress</option>
                        <option value="Lightings">Lightings</option>
                        <option value="Tableware">Tableware</option>
                        <option value="Hardware & Automations">Hardware & Automations</option>
                        <option value="Others">Others</option>
                      </select>
                    </div>
                    <div class="form-group">
                      <input type="text" class="form-control" name="city" id="city" placeholder="City" required>
                    </div>

                    <button type="submit" class="btn btn-5 w-100p mt-20">Get Quote</button>
                  </form>

                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

