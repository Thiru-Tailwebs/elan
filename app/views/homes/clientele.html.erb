<%= render "shared/header" %>

<section class="bg-images" style="background-image: url('/images/brand/clientele-banner.png');">
  <div class="bg-shadow-1 h-90vh h-80vh-lg h-80vh-md h-80vh-mob">
    <div class="container">
      <div class="h-90vh h-80vh-lg h-80vh-md h-80vh-mob d-flex align-items-center justify-content-center">
        <div class="zoom">
          <div data-aos="fade-up" class="heading-4 white fs-26 fs-18-mob text-upper text-center">
            Clientele
          </div>
          <div data-aos="fade-up" class="mt-15 w-70p w-90p-mob mx-auto fs-35-mob heading-1 text-center">
            Where Excellence Meets Partnership
          </div>
          <div data-aos="fade-up" class="mt-30 w-70p w-90p-mob mx-auto fs-14-mob heading-3 white text-center">
            Forging Futures Through Partnership, A Journey of Excellence and Collaboration with Our Esteemed Clientele.
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

<section class="sandal-bg py-50 zoom">
  <div class="container">
    <div class="heading-8 fs-18 w-80p w-100p-mob text-center mx-auto" data-aos="fade-down">
      Discover the stories of businesses that soared to new heights with our collaborative approach, tailored solutions, and
      unwavering commitment.
    </div>
    <div class="position-relative mt-50" data-aos="zoom-out">
      <img src="/images/brand/client-1.png" class="w-100p img-overlay">
      <div class="heading-1 text-overlay text-center fs-20-mob">Our Clientele, Our Pride</div>
    </div>
  </div>
</section>

<section class="py-50 zoom">
  <div class="container">
  <div class="mx-50 mx-0-mob">
    <div class="row align-items-center">
<!--     <div class="row align-items-center"> -->
      <% @clientele_image.each do |data| %>
        <div class="col-md-3 col-4 mt-50 mt-0-mob text-center" data-aos="zoom-in">
          <img src="<%= data.image_url(:medium) %>" class="w-80p">
        </div>
      <% end %>
    </div>
    <div class="mt-80 mt-40-mob mt-40-md mt-40-lg text-center" data-aos="flip-down">
      <a href="<%= brands_path %>" type="button" class="btn btn-2">brands we house</a>
    </div>
  </div>
  </div>
</section>

<%= render "shared/call_back" %>

<%= render "shared/footer" %>