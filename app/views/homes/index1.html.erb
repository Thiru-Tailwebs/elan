    .owl-carousel.owl-2 .owl-item img {
      width: 50%;
    }

    .owl-carousel.owl-2 .owl-item .img-center {
      display: flex;
      justify-content: center;
    }

    .owl-carousel.owl-2 .owl-nav button.owl-prev {
      position: absolute;
      left: 0;
      top: 35%;
    }

    .owl-carousel.owl-2 .owl-nav button.owl-next {
      position: absolute;
      right: 0;
      top: 35%;
    }

    .sr-fade.sr .animated-text {
      animation: fade 1s ease-in-out;
    }

    .sl-fade.sr .animated-text {
      animation: fade 1s ease-in-out;
    }

    @keyframes fade {
      from {
        opacity: 0;
        transform: translate(0, 10px);
      }

      to {
        opacity: 1;
        transform: translate(0, 0);
      }
    }

    .animated-text {
      position: relative;
      overflow: hidden;
    }

    .animated-text .animation {
      display: inline-block;
      white-space: nowrap;
    }

    .animated-text .word {
      display: inline-block;
      opacity: 0;
      transform: translateY(100%);
      transition: opacity 0.6s ease, transform 0.6s ease;
    }

    .animated-text .word.active {
      opacity: 1;
      transform: translateY(0);
    }

