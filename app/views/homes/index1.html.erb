      $(".owl-1 .owl-nav .owl-prev").html('<img src="/images/home-page/owl-1l.png" class="w-40px">');
      $(".owl-1 .owl-nav .owl-next").html('<img src="/images/home-page/owl-1r.png" class="w-40px">');
    });

    $(document).ready(function () {
      $(".owl-2").owlCarousel({
        items: 1,
        margin: 10,
        loop: true,
        nav: true,
        dots: false
      });
      $(".owl-2 .owl-nav .owl-prev").html('<img src="/images/home-page/owl-1l.png" class="w-40px">');
      $(".owl-2 .owl-nav .owl-next").html('<img src="/images/home-page/owl-1r.png" class="w-40px">');
    });

    $(document).ready(function () {
      $(".owl-3").owlCarousel({
        items: 3,
        margin: 20,
        loop: true,
        nav: true,
        dots: false
      });
    });

    // Initialize ScrollReveal
    ScrollReveal({
      duration: 800, // Duration of the animation (in milliseconds)
      distance: '20px', // Distance the element moves during animation
      origin: 'bottom', // Origin of the animation
      easing: 'ease-in-out', // Easing function
      interval: 100, // Time between elements animating sequentially
      viewFactor: 1,
      delay: 1200
    });

    ScrollReveal().reveal('.sr-fade', {
      delay: 1200,
      distance: '50px',
      origin: 'right',
      interval: 200,
      viewFactor: 1,
    });

    ScrollReveal().reveal('.sl-fade', {
      delay: 1200,
      distance: '50px',
      origin: 'left',
      interval: 200,
      viewFactor: 1,
    });

    // Initialize ScrollReveal
    ScrollReveal().reveal('.animated-text', {
      delay: 1500,
      distance: '50px',
      origin: 'bottom',
      duration: 700,
      opacity: 0,
      easing: 'ease-in-out',
      afterReveal: (el) => {
        const words = el.querySelectorAll('.word');
        let delay = 0;

        words.forEach((word, index) => {
          setTimeout(() => {
            word.classList.add('active');
          }, delay);
          delay += 200; // Adjust the delay between each word
        });
      },
    });

    ScrollReveal().reveal('.w-70p', {
      duration: 700,      // Animation duration in milliseconds
      distance: '50px',  // Distance the image travels before it appears
      delay: 1500,        // Delay before the animation starts (milliseconds)
    });

    // Customize the animation for each image
    ScrollReveal().reveal('.w-70p:nth-child(1)', {
      origin: 'left'    // Image 1 comes from the left
    });
    ScrollReveal().reveal('.w-70p:nth-child(2)', {
      origin: 'right'   // Image 2 comes from the right
    });
    ScrollReveal().reveal('.w-70p:nth-child(3)', {
      origin: 'top'     // Image 3 comes from the top
    });
    ScrollReveal().reveal('.w-70p:nth-child(4)', {
      origin: 'bottom'  // Image 4 comes from the bottom
    });


