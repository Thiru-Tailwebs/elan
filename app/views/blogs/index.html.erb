    .form-group {
      margin-top: 30px !important;
    }

    .custom-text-box {
      height: 150px;
      /* Customize the height as needed */
    }

    /* Custom checkbox style */
    .custom-checkbox .custom-control-label::before {
      border: 1px solid #2D2D2D;
      /* Change the border color to your desired color */
    }

    .custom-checkbox .custom-control-input:checked~.custom-control-label::before {
      background-color: #2D2D2D;
      /* Change the background color to your desired color when checked */
      border-color: #2D2D2D;
      /* Change the border color when checked */
    }

    /* Optional: Change the label text color */
    .custom-checkbox .custom-control-label {
      color: #3A3A3A;
      font-family: 'Jost', sans-serif;
      font-size: 16px;
      font-style: normal;
      font-weight: 500;
      line-height: normal;
